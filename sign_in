<script>
  //<![CDATA[
    $(document).ready(function() {
      $(".helpdesk_link").click(function() {
        try {
          Zenbox.show(); 
        } catch (err) {
          $.ajax({ 
            url: $("#root_path").text() + '/zendesk', 
            dataType: 'html',  
            sync: true,
            success: function(data) { $(data).appendTo("body");}, 
            error: function() { 
              window.location.href = "http://bitnami.zendesk.com/account/dropboxes/20076392"; 
            }
          });
        }
        return false;
      });
    });
  //]]>
</script>
